# Email Configuration (Required for contact form)
# Get your API key from https://resend.com
RESEND_API_KEY=your_resend_api_key_here

# Your email address to receive contact form messages
CONTACT_EMAIL=your-email@example.com

# Email sender address (optional, defaults to Resend's onboarding domain)
FROM_EMAIL=Resume Contact <onboarding@resend.dev>

# AI Demos Configuration (Optional)
# Enable real AI-powered demos (default: offline heuristics)
NEXT_PUBLIC_OPENAI_ENABLED=false
OPENAI_API_KEY=your_openai_api_key_here

# Analytics (Optional)
# Vercel Analytics is enabled by default when deployed to Vercel
VERCEL_ANALYTICS_ID=your_vercel_analytics_id